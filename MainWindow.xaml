<Window x:Class="DiagnoseMe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiagnoseMe"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <ScrollViewer x:Name="outPut" FontFamily="Lucida Console">
            <StackPanel x:Name="stackPanel" HorizontalAlignment="Left" VerticalAlignment="Top">
                <TextBox x:Name="outText" TextWrapping="Wrap" Margin="20"/>
            </StackPanel>
        </ScrollViewer>
        <TextBox x:Name="clientCode" FontFamily="Lucida Console" Width="128" Height="23" Text="Client code here"/>

    </Grid>
</Window>
